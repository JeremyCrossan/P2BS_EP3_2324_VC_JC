
<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <VBox id="_IDGenVBox1">
        <Image id="_IDGenImage1" src="{picture}" width="100px" alt="{naam}" press="onEventClick" />
        <Text id="_IDGenText1" text="{naam}" />
        <Text id="_IDGenText2" text="{datum}" />
        <Text id="_IDGenText3" text="{locatie}" />
        <Button id="_IDGenButton1" text="Meer informatie" press="onMoreInfoPress" />
    </VBox>
</core:FragmentDefinition>
